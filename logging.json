    "formatters": {
        "default": {
            "format": "%(asctime)s %(levelname)-8s - %(message)s",
            "datefmt": "%H:%M:%S"
        }
    },
    "handlers": {

    },
    "loggers": {
        "game_io": {
            "level": "DEBUG"
        },
        "tk_io": {
